# SMContent Environment Variables
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-role-key
DATABASE_URL=postgresql://user:password@host:port/database

# Application Settings
USE_SUPABASE=true
DATA_FOLDER=data

# Qdrant Configuration (for vector database)
QDRANT_URL=localhost:6333
QDRANT_API_KEY=

# ML Model Settings (defaults are usually fine)
EMBEDDING_MODEL_ID=sentence-transformers/all-MiniLM-L6-v2
CROSS_ENCODER_MODEL_ID=cross-encoder/ms-marco-MiniLM-L-6-v2
EMBEDDING_MODEL_MAX_INPUT_LENGTH=256
EMBEDDING_SIZE=384
EMBEDDING_MODEL_DEVICE=cpu
VECTOR_DB_OUTPUT_COLLECTION_NAME=linkedin_posts